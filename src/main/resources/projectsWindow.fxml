        <ScrollPane id="scrollPane" hbarPolicy="NEVER" prefHeight="155.0" prefWidth="400.0">
            <content>
                <VBox id="vbox" fx:id="vBox" />
            </content>
        </ScrollPane>
      <Button id="updateAllButton" layoutX="322.0" layoutY="163.0" mnemonicParsing="false" onAction="#updateAllClicked" text="Update all" />
      <Button id="addButton" layoutX="322.0" layoutY="204.0" mnemonicParsing="false" onAction="#addNewClicked" prefHeight="25.0" prefWidth="71.0" text="Add new" />
      <Button id="updateSelectedButton" layoutX="200.0" layoutY="163.0" mnemonicParsing="false" onAction="#updateSelectedClicked" text="Update selected" />
      <Button id="deleteSelectedButton" layoutX="200.0" layoutY="204.0" mnemonicParsing="false" onAction="#deleteSelectedClicked" prefHeight="25.0" prefWidth="102.0" text="Delete selected" />
    </children>
</AnchorPane>
